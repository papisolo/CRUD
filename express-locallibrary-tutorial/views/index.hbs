<div class="row">
    <div class="col -6"><h1>USERS</h1></div>
    <div class="col -6">
        <a  type = "button" class = "btn btn-primary btn-small" href="user">+Create user </a>
    </div>
</div>


<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone Number</th>
      <th scope="col">Delete/Edit</th>
      </tr>
  </thead>
  <tbody>
      {{#each data}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.First_Name}}</td>
      <td>{{this.Last_Name}}</td>
      <td>{{this.Email}}</td>
      <td>{{this.Phone_Number}}</td>
      <td>
      <a href= "/{{this.id}}" type = "button" class = "btn btn-light btn-small"  ><i class = "bi bi-person-x"></i>Delete</a>
      <a href= "/edit/{{this.id}}/{{this.First_Name}}" type = "button" class = "btn btn-light btn-small"><i class = "bi bi-pencil"></i>Edit</a>
      <a href= "#" type = "button" class = "btn btn-light btn-small"><i class = "bi bi-eye"></i>View</a></td>
      

      
      
      

    </tr>
    {{/each}}
    
  </tbody>
</table>